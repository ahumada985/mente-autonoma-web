{
  "version": 2,
  "buildCommand": "prisma generate && npm run build",
  "installCommand": "npm install && npx prisma generate",
  "functions": {
    "api/chatbot.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/chatbot",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}